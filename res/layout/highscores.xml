<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:background="@drawable/menubackground3" android:orientation="vertical">
	<TextView android:id="@+id/textView1" android:layout_width="fill_parent"
		android:gravity="center" android:text="HighScores" android:textColor="#ffffff"
		android:textStyle="bold" android:layout_height="33dip"
		android:textSize="24dip"></TextView>
	<ListView android:layout_width="match_parent" android:id="@+id/ListViewScores"
		android:layout_height="fill_parent" android:layout_marginBottom="33dip"
		android:layout_marginLeft="33dip" android:layout_marginRight="33dip"></ListView>
</LinearLayout>
